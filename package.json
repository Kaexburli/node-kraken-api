{
  "name": "node-kraken-api",
  "version": "1.0.0",
  "description": "a typed REST/WS Node.JS client for the Kraken cryptocurrency exchange",
  "license": "MIT",
  "repository": "git://github.com/jpcx/node-kraken-api.git",
  "author": {
    "name": "Justin Collier",
    "email": "m@jpcx.dev",
    "url": "https://github.com/jpcx"
  },
  "keywords": [
    "kraken",
    "REST",
    "WS",
    "api",
    "cryptocurrency",
    "bitcoin",
    "litecoin",
    "ethereum",
    "exchange",
    "trading"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "index.d.ts",
    "index.js",
    "package-lock.json",
    "package.json"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "clean": "rm -f index.js; rm -f index.d.ts; rm -rf test/.build; rm -rf codegen/.build",
    "build": "tsc --pretty -p .",
    "test": "npm run clean && tsc --pretty -p test && npx testts test/.build/test/test.js",
    "codegen": "npm run clean && tsc --pretty -p codegen && sh codegen/codegen.sh",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "ws": "^8.2.2",
    "crc": "^3.8.0"
  },
  "devDependencies": {
    "typescript": "^4.4.3",
    "@types/ws": "^7.4.7",
    "@types/crc": "^3.4.0",
    "@types/node": "^16.9.1",
    "@jpcx/testts": "^0.4.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.5"
  }
}
